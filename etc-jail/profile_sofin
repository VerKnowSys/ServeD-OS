# CC:
export CC='/usr/bin/clang '

# CXX:
export CXX='/usr/bin/clang++ '

# CPP:
export CPP='/usr/bin/clang-cpp'

# PATH:
export PATH=/Software/Zsh/exports:/Software/Git/exports:/bin:/usr/bin:/sbin:/usr/sbin

# CFLAGS:
export CFLAGS='-I/Software/Zsh/include -I/Software/Git/include  -fPIC -Os -fPIC -fPIE -fno-strict-overflow -fstack-protector-all'

# CXXFLAGS:
export CXXFLAGS='-I/Software/Zsh/include -I/Software/Git/include  -fPIC -Os -fPIC -fPIE -fno-strict-overflow -fstack-protector-all'

# LDFLAGS:
export LDFLAGS='-L/Software/Zsh/lib -L/Software/Git/libexec -L/Software/Git/lib  -fPIC -fPIE -Wl,--enable-new-dtags'

# PKG_CONFIG_PATH:
export PKG_CONFIG_PATH='/Software/Zsh/lib/pkgconfig:/Software/Git/lib/pkgconfig:'

# MANPATH:
export MANPATH='/Software/Zsh/share/man:/Software/Git/share/man:/Software/Git/man:/usr/share/man:/usr/share/openssl/man'